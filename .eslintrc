{
  "extends": "airbnb/base",
  "rules":  {
    "arrow-body-style": [2, "always"],
    "arrow-spacing": 2,
    "comma-dangle": [2, "never"],
    "eqeqeq": [2, "allow-null"],
    "func-names": 0,
    "indent": [2, 2, {"SwitchCase": 0}],
    "max-len": [2, 117, 2],
    "no-alert": 0,
    "no-console": 0,
    "no-constant-condition": 2,
    "no-debugger": 2,
    "no-eq-null": 0,
    "no-extra-strict": 0,
    "no-undef": 2,
    "no-unused-expressions": 0,
    "object-curly-spacing": [2, "always"],
    "space-in-parens": [2, "never"],
    "strict": [2, "function"]
  },
  "ecmaFeatures": {
    "modules": true,
    "generators": true
  },
  "globals": {
    "expect": true,
    "AWS": true
  },
  "env": {
    "worker": true,
    "jquery": false,
    "node": true,
    "commonjs": true,
    "es6": true
  }
}
